    private void gerarMouseClicked(java.awt.event.MouseEvent evt) {                                   
        // TODO add your handling code here:
     resultado.setText("Nome: "+nome.getText()+"\nIdade: "+idade.getText()+"\nCPF: "+cpf.getText()+"\nCidade: "+cidade.getText()+"\nUF: "+uf.getSelectedItem()+"\nLogradouro: "+logradouro.getText()+"\nBairro: "+bairro.getText()+"\nSexo: "+sexo.getSelectedItem());
    }                                  

